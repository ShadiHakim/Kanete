<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kanete">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kanete">
        <activity
            android:name=".Customer.CustomerMainActivity"
            android:exported="true" />
        <activity
            android:name=".Store.StoreMainActivity"
            android:exported="true" />
        <activity
            android:name="com.example.kanete.AuthenticationActivities.SignupActivity"
            android:exported="true" />
        <activity
            android:name="com.example.kanete.AuthenticationActivities.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>